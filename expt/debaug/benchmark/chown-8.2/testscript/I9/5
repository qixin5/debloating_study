#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

uname=$(whoami)

{ timeout -k 9 ${TIMEOUT}s $BIN ${uname}:sudo /TestUnix; } &>$OUTDIR/o5

echo "$?" >>$OUTDIR/o5
own0=`ls -al /TestUnix | tr -s ' ' | cut -d ' ' -f 3`
own1=`ls -al /TestUnix | tr -s ' ' | cut -d ' ' -f 4`
echo "${own0} ${own1}" >>$OUTDIR/o5

chown -R qxin6:qxin6 /TestUnix
