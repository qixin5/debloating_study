#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

cp -r $INDIR/myarchive.tar ./

{ timeout -k 9 ${TIMEOUT}s $BIN -xvf myarchive.tar; } &>$OUTDIR/o4

echo "$?" >>$OUTDIR/o4

diff home/qxin6/debaug_expt/debaug/benchmark/tar-1.14/input.origin/I7/etc/file $INDIR/etc/file
echo "$?" >>$OUTDIR/o4

diff home/qxin6/debaug_expt/debaug/benchmark/tar-1.14/input.origin/I7/anaconda-ks.cfg $INDIR/anaconda-ks.cfg
echo "$?" >>$OUTDIR/o4



