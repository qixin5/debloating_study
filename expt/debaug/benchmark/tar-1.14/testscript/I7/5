#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4

cp -r $INDIR/myarchive.tar ./
mkdir tmp

{ timeout -k 9 ${TIMEOUT}s $BIN -xvf myarchive.tar -C tmp; } &>$OUTDIR/o5

echo "$?" >>$OUTDIR/o5

if [ -f tmp/default.profraw ]; then
    cp tmp/default.profraw ./
fi

diff tmp/home/qxin6/debaug_expt/debaug/benchmark/tar-1.14/input.origin/I7/etc/file $INDIR/etc/file
echo "$?" >>$OUTDIR/o5

diff tmp/home/qxin6/debaug_expt/debaug/benchmark/tar-1.14/input.origin/I7/anaconda-ks.cfg $INDIR/anaconda-ks.cfg
echo "$?" >>$OUTDIR/o5


