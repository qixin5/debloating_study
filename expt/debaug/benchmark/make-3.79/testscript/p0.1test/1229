#!/bin/bash

BIN=$1
OUTDIR=$2
TIMEOUT=$3
INDIR=$4
CURRDIR=$(pwd)

cp -r -p $INDIR/sdir-other3 ./sdir
cd sdir
timeout -k 9 ${TIMEOUT}s $BIN -p -f/dev/null > $OUTDIR/o1229 2>&1
${DEBSTUDY_DIR}/expt/debaug/benchmark/make-3.79_template/testscript/util/rm-makestuff -p $OUTDIR/o1229
${DEBAUG_DIR}/bin/makeoutputcleanup $OUTDIR/o1229
if [ -f default.profraw ]; then
    mv default.profraw ${CURRDIR}/
fi

cd ..
rm -r -f sdir
